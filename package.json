{
  "name": "@stevensacks/react-project-template",
  "description": "React Project Template",
  "repository": "git://github.com/stevensacks/react-project-template.git",
  "version": "0.0.1",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "authors": [
    "Steven Sacks <stevensacks@gmail.com>"
  ],
  "scripts": {
    "prepare": "is-ci || husky install",
    "lint": "eslint --fix --max-warnings=0 --ext js,jsx,ts,tsx,json src test",
    "lint:tsc": "tsc --noEmit",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.{ts,tsx,html,md,mdx,css}\"",
    "stylelint": "stylelint --fix src/**/*.css",
    "lint-all": "npm run lint:tsc && npm run lint && npm run format && npm run stylelint",
    "lint:ci": "eslint --max-warnings=0 --ext js,jsx,ts,tsx src && stylelint src/**/*.css",
    "lint:dry": "eslint --ext js,jsx,ts,tsx src test",
    "test": "is-ci-cli test:ci test:cw",
    "test:ci": "cross-env NODE_ENV=test jest",
    "test:cw": "cross-env NODE_ENV=test jest --coverage --watch",
    "test:cwa": "cross-env NODE_ENV=test jest --coverage --watchAll",
    "test:bs": "browser-sync start -s --port 5150 -f 'coverage/lcov-report/index.html' --startPath 'coverage/lcov-report'",
    "test:lint-staged": "cross-env NODE_ENV=test jest --bail --onlyChanged --passWithNoTests",
    "test:clear": "jest --clearCache",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "clean-storybook": "rimraf node_modules/.cache",
    "build-storybook": "cross-env SB=1 build-storybook",
    "chromatic": "cross-env SB=1 npx chromatic --only-changed --exit-once-uploaded --exit-zero-on-changes --skip '@(renovate/**|dependabot/**)'"
  },
  "lint-staged": {
    "{src,test}/**/*.{ts,tsx}": [
      "eslint --cache --fix --max-warnings=0",
      "prettier --write"
    ],
    "src/**/*.css": [
      "stylelint --fix",
      "prettier --write"
    ],
    "src/**/*.json": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.7.9",
    "@fortawesome/fontawesome-svg-core": "6.3.0",
    "@fortawesome/free-brands-svg-icons": "6.3.0",
    "@fortawesome/free-regular-svg-icons": "6.3.0",
    "@fortawesome/free-solid-svg-icons": "6.3.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "clsx": "1.2.1",
    "focus-visible": "5.2.0",
    "graphql": "16.6.0",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "spark-md5": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.0",
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@graphql-codegen/cli": "3.2.1",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/typescript": "3.0.1",
    "@graphql-codegen/typescript-operations": "3.0.1",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-docs": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "6.5.16",
    "@storybook/testing-react": "1.3.0",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/line-clamp": "0.4.2",
    "@testing-library/dom": "9.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/spark-md5": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "auto": "10.42.2",
    "autoprefixer": "10.4.13",
    "babel-jest": "29.4.3",
    "babel-loader": "9.1.2",
    "browser-sync": "2.27.12",
    "chromatic": "6.17.1",
    "commitizen": "4.3.0",
    "cross-env": "7.0.3",
